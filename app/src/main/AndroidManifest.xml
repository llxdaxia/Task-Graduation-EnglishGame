<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.zhu.cainiao">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.BIND_TELECOM_CONNECTION_SERVICE" />

    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".module.RegisterActivity"
            android:label="注册" />
        <activity
            android:name=".module.LoginActivity"
            android:label="登录" />
        <activity android:name=".module.CheckpointsActivity" />
        <activity
            android:name=".module.StudyActivity"
            android:label="学习" />
        <activity
            android:name=".module.SetActivity"
            android:label="个人设置" />
        <activity
            android:name=".module.TaskExplainActivity"
            android:label="任务说明" />
        <activity
            android:name=".module.CheckpointsLevelActivity"
            android:label="通关情况" />
        <activity
            android:name=".module.StudyLevelActivity"
            android:label="学习情况" />
    </application>

</manifest>